import matplotlib.pyplot as plt

sharpaccuracy = 1.00
blurred1accuracy = (1.00 + 0.99 + 1.00 + 1.00) / 4
blurred2accuracy = (0.96 + 0.92 + 0.94 + 0.97) / 4
blurred3accuracy = (0.62 + 0.57 + 0.56 + 0.60) / 4
blurred4accuracy = (0.21 + 0.17 + 0.22 + 0.22) / 4
blurred5accuracy = (0.05 + 0.09 + 0.08 + 0.05) / 4

plt.plot([0,1,2,3,4,5], [sharpaccuracy,blurred1accuracy,blurred2accuracy,blurred3accuracy,blurred4accuracy,blurred5accuracy], 'ro', linestyle='-')
plt.axis([0, 6, 0, 1.01])
plt.show()
